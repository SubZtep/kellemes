{
  "name": "@kellemes/cli",
  "version": "1.0.0",
  "author": "Andras Serfozo <subztep@gmail.com>",
  "description": "Interactive CLI for keLLeMes",
  "license": "BSD-3-Clause",
  "private": true,
  "type": "module",
  "bin": {
    "kellemes-cli": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx --tsconfig tsconfig.json src/index.tsx",
    "build": "tsc",
    "sea:generate": "node --experimental-sea-config build/sea.config.json",
    "xxsea:inject": "cp $(command -v node) dist/app && npx postject dist/app NODE_SEA_BLOB dist/app.blob --sentinel-fuse node:sea",
    "sea:inject": "node build/inject.mjs",
    "sea:build": "npm run build && npm run sea:generate && npm run sea:inject",
    "start": "node dist/index.js",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@kellemes/common": "workspace:*",
    "@kellemes/rag": "workspace:*",
    "@pppp606/ink-chart": "^0.2.0",
    "@tanstack/react-query": "^5.90.11",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "ink": "^6.5.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "ollama": "^0.6.3",
    "react": "^19.2.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "postject": "1.0.0-alpha.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
